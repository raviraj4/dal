png("residuals_hist.png", width = 800, height = 800)
hist(residuals, main = "Hist Of Residuals", xlab = "RESIDUALS", col = "lightblue", border = "white")
dev.off()
results <- data.frame(Actual=test_data$Revenue, Predicted=prediction_test)
