# Read the dataset
df <- read.csv("StudentsPerformance.csv") # Or whatever your file is named
# -----------------------------------------------
# 1. Initial Exploration
# -----------------------------------------------
cat("--- 1. DIMENSIONS AND SIZE ---\n")
cat("Dataset Dimensions (Rows, Columns):", dim(df), "\n")
cat("Column Names:\n")
print(names(df))
cat("\nFirst few rows:\n")
print(head(df))
# -----------------------------------------------
# 2. Structure and Variable Types
# -----------------------------------------------
cat("\n--- 2. STRUCTURE AND VARIABLE TYPES ---\n")
str(df)
# -----------------------------------------------
# 3. Comprehensive Overview
# -----------------------------------------------
cat("\n--- 3. COMPREHENSIVE OVERVIEW ---\n")
print(skim(df))
# -----------------------------------------------
# 4. Frequencies and Mode
# -----------------------------------------------
cat("\n--- 4. FREQUENCIES AND MODE ---\n")
# Function to calculate mode
get_mode <- function(v) {
uniqv <- unique(v)
uniqv[which.max(tabulate(match(v, uniqv)))]
}
# Analyze categorical variables
categorical_vars <- names(df)[sapply(df, is.character)]
for (var in categorical_vars) {
cat("\nFrequency table for", var, ":\n")
freq_table <- table(df[[var]])
print(freq_table)
cat("Mode:", names(freq_table)[which.max(freq_table)], "\n")
}
# -----------------------------------------------
# 5. Percentiles for Numerical Variables
# -----------------------------------------------
cat("\n--- 5. PERCENTILES ---\n")
numerical_vars <- names(df)[sapply(df, is.numeric)]
for (var in numerical_vars) {
cat("\nPercentiles for", var, ":\n")
percentiles <- quantile(df[[var]], probs = c(0, 0.25, 0.5, 0.75, 1), na.rm = TRUE)
print(percentiles)
}
# -----------------------------------------------
# 6. Measures of Central Tendency
# -----------------------------------------------
cat("\n--- 6. CENTRAL TENDENCY ---\n")
for (var in numerical_vars) {
cat("\nVariable:", var, "\n")
cat("Mean:", mean(df[[var]], na.rm = TRUE), "\n")
cat("Median:", median(df[[var]], na.rm = TRUE), "\n")
}
# -----------------------------------------------
# 7. Measures of Spread
# -----------------------------------------------
cat("\n--- 7. MEASURES OF SPREAD ---\n")
for (var in numerical_vars) {
cat("\nVariable:", var, "\n")
cat("Range:", range(df[[var]], na.rm = TRUE), "\n")
cat("Variance:", var(df[[var]], na.rm = TRUE), "\n")
cat("Standard Deviation:", sd(df[[var]], na.rm = TRUE), "\n")
cat("IQR:", IQR(df[[var]], na.rm = TRUE), "\n")
}
# -----------------------------------------------
# 8. Measures of Shape
# -----------------------------------------------
cat("\n--- 8. MEASURES OF SHAPE ---\n")
for (var in numerical_vars) {
cat("\nVariable:", var, "\n")
cat("Skewness:", skewness(df[[var]], na.rm = TRUE), "\n")
cat("Kurtosis:", kurtosis(df[[var]], na.rm = TRUE), "\n")
}
# -----------------------------------------------
# BONUS: Quick Visualizations
# -----------------------------------------------
cat("\n--- BONUS: GENERATING BASIC PLOTS ---\n")
# Histograms for numerical variables
for (var in numerical_vars) {
hist(df[[var]], main = paste("Distribution of", var),
xlab = var, col = "lightblue", border = "black")
}
# Bar plots for categorical variables (first 3)
if (length(categorical_vars) > 0) {
for (i in 1:min(3, length(categorical_vars))) {
barplot(table(df[[categorical_vars[i]]]),
main = paste("Distribution of", categorical_vars[i]),
las = 2) # Rotate x-axis labels
}
}
# Boxplots for numerical variables
for (var in numerical_vars) {
boxplot(df[[var]], main = paste("Boxplot of", var), ylab = var)
}
cat("\n--- ANALYSIS COMPLETE ---\n")
